    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^6.6.1",
    "helmet": "^3.23.3",
