{
    "env": {
        "browser": false,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "prettier/prettier": ["error"],
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "off",
            "windows"
        ]
    }
}